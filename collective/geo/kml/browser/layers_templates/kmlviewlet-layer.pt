function(){var layer=new OpenLayers.Layer.GML('%s','%s'+'@@kml-document',{format:OpenLayers.Format.KML,projection:cgmap.createDefaultOptions().displayProjection,formatOptions:{extractStyles:true,extractAttributes:true}});layer.events.on({"loadend":function(){layer.map.zoomToExtent(layer.getDataExtent());if(layer.features.length>1){layer.map.zoomTo(layer.map.getZoom()-1)}else{layer.map.zoomTo(layer.map.getZoom()-4)}}});return layer}
