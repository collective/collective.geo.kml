<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <object name="document_actions" meta_type="CMF Action Category">
  <object name="kml_download" meta_type="CMF Action" i18n:domain="plone" insert-after="*">
   <property name="title" i18n:translate="">Download KML</property>
   <property name="description" i18n:translate="">Download the KML file associated with this content</property>
   <property name="url_expr">string:$object_url/@@kml-document</property>
   <property name="icon_expr"></property>
   <property
      name="available_expr">python:object.getLayout() == 'kml-openlayers' or object.restrictedTraverse('@@geoview').isGeoreferenceable()</property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
</object>
